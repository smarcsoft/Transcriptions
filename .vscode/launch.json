{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Service in test mode",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": ["--test", "start"],
            "console": "integratedTerminal"
        },
        {
            "name": "User Interface",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": ["--file","testfiles\\sample.m4a", "--stdout"],
            "console": "integratedTerminal"
        }
    ]
}